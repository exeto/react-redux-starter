{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "private": true,
  "description": "React Redux Starter",
  "license": "MIT",
  "repository": "exeto/react-redux-starter",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "author": "Timofey Dergachev <t@exeto.me> (https://exeto.me/)",
  "scripts": {
    "test": "npm run lint:js && npm run lint:scss",
    "start": "npm run clean && BABEL_ENV=client webpack-dashboard -- node devServer",
    "start:prod": "NODE_ENV=production node tmp/server.bundle.js",
    "lint:js": "eslint --ext jsx,js .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint -s scss client/**/*.scss",
    "clean": "rm -rf public tmp",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production BABEL_ENV=client webpack --progress --config webpack/client.prod.js",
    "build:server": "NODE_ENV=production BABEL_ENV=server webpack --progress --config webpack/server.prod.js"
  },
  "dependencies": {
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.1.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^4.0.0",
    "redux": "^3.1.2",
    "redux-actions": "^0.11.0"
    "redux-apist": "^0.2.0",
    "redux-thunk": "^2.1.0",
    "s-ago": "^1.1.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "browser-sync": "^2.11.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.2.2",
    "eslint-config-exeto": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "lodash.mergewith": "^4.3.5",
    "node-sass": "^3.7.0",
    "postcss-csso": "^1.1.1",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-loader": "^0.10.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.0.2",
    "stylelint-config-standard": "^12.0.0",
    "webpack": "^1.12.12",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
