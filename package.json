{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "private": true,
  "description": "React Redux Starter",
  "license": "MIT",
  "repository": "exeto/react-redux-starter",
  "engines": {
    "node": "^5.0.0"
  },
  "author": "Timofey Dergachev <t@exeto.me> (https://exeto.me/)",
  "scripts": {
    "test": "npm run lint:js && npm run lint:scss",
    "start": "npm run clean && BABEL_ENV=client webpack-dashboard -- node devServer",
    "start:prod": "NODE_ENV=production node tmp/server.bundle.js",
    "lint:js": "eslint --ext jsx,js .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint -s scss client/**/*.scss",
    "clean": "rm -rf public tmp",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production BABEL_ENV=client webpack --progress --config webpack/client.prod.js",
    "build:server": "NODE_ENV=production BABEL_ENV=server webpack --progress --config webpack/server.prod.js"
  },
  "dependencies": {
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.0.0",
    "redux-apist": "^0.2.0",
    "redux-thunk": "^2.1.0",
    "s-ago": "^1.1.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-latest-node5": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.15.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.5.0",
    "eslint-config-exeto": "^11.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "lodash.mergewith": "^4.6.0",
    "node-sass": "^3.9.3",
    "postcss-csso": "^1.1.2",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-loader": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^14.0.0",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  }
}
