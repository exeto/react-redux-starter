{
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "modules": false
  },
  "rules": {
    "strict": [2, "global"],
    "no-console": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true } }],
    "no-shadow": [2, { "allow": ["err", "resolve", "reject", "done", "cb"] }]
  }
}
